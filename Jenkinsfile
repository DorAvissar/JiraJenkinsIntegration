pipeline {
    agent any // Use any available Jenkins agent
    stages {
        stage('Hello World1') { // Name of the build stage
            steps {
                // A simple build step that prints "Hello World" to the console
                sh 'echo "Hello World!"'
            }
        }
    }
}


